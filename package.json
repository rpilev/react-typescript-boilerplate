{
  "name": "react-typescript-boilerplate",
  "version": "1.0.1",
  "description": "React Typescript Boilerplate",
  "main": "./src/index.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --content-base ./public --port 3001",
